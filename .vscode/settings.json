{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "notebook.codeActionsOnSave": {
      "notebook.source.fixAll": "explicit",
      "notebook.source.organizeImports": "explicit"
    }
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "ansible.python.interpreterPath": "${workspaceFolder}/.venv/bin/python",
  "cSpell.ignorePaths": [
    ".git",
    ".github",
    ".venv",
    ".vscode",
    "requirements.lock",
    "requirements.txt"
  ],
  "cSpell.words": [
    "activerun",
    "arange",
    "autobuild",
    "autofix",
    "autoremove",
    "bibtex",
    "boto",
    "buildx",
    "ceph",
    "chartmuseum",
    "chdir",
    "citeproc",
    "ckpt",
    "cuda",
    "cudnn",
    "cumsum",
    "cython",
    "dataframe",
    "datamodule",
    "dockerfiles",
    "dockle",
    "dotenv",
    "envlist",
    "fastapi",
    "figsize",
    "forcerm",
    "gitbranch",
    "gitdiff",
    "githash",
    "gitwildmatch",
    "Growi",
    "gunicorn",
    "hadolint",
    "iframe",
    "imshow",
    "ipynb",
    "isort",
    "joblib",
    "kakera",
    "kubeconfig",
    "Kubeflow",
    "kubelet",
    "Kubespray",
    "Kustomize",
    "linebreak",
    "localk8s",
    "markdownlint",
    "matplotlib",
    "mattermostdriver",
    "maxdepth",
    "mccabe",
    "mdfile",
    "metallb",
    "minio",
    "mlflow",
    "mlproject",
    "mlruns",
    "mpld",
    "multirun",
    "multiversion",
    "mypy",
    "nbformat",
    "ndarray",
    "noninteractive",
    "noqa",
    "nsga",
    "NSGAII",
    "NSGAIII",
    "numba",
    "numpy",
    "omegaconf",
    "optuna",
    "Pandoc",
    "pathlib",
    "plesk",
    "plotly",
    "psutil",
    "pycodestyle",
    "pydantic",
    "pydocstyle",
    "pyenv",
    "pyfunc",
    "pylint",
    "pymysql",
    "pynvml",
    "pyproject",
    "pytest",
    "PYTHONHASHSEED",
    "pyupgrade",
    "quickstart",
    "randn",
    "resolv",
    "scikit",
    "scipy",
    "seaborn",
    "secho",
    "setuptools",
    "shfmt",
    "subdir",
    "templating",
    "tenumerate",
    "tflint",
    "tinydb",
    "toctree",
    "torchvision",
    "tqdm",
    "UMAP",
    "undoc",
    "unsqueeze",
    "uvicorn",
    "venv",
    "virtualenvs",
    "webui",
    "zmax",
    "zmin",
    "Zotero"
  ],
  "editor.tabSize": 4,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.ansible.yaml": "ansible",
    "*.lock": "pip-requirements",
    "*.rst_t": "jinja"
  },
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "material-icon-theme.activeIconPack": "react",
  "notebook.insertFinalNewline": true,
  "python.envFile": "${workspaceFolder}/.venv",
  "python.testing.pytestEnabled": true,
  "redhat.telemetry.enabled": false,
  "ruff.configuration": "${workspaceFolder}/ruff.toml",
  "ruff.configurationPreference": "filesystemFirst",
  "ruff.enable": true,
  "ruff.importStrategy": "fromEnvironment",
  "terminal.integrated.profiles.linux": {
    "pytmp": {
      "args": [
        "-c",
        "source ${workspaceFolder}/.vscode/setup ${workspaceFolder} && exec bash"
      ],
      "color": "terminal.ansiCyan",
      "env": {},
      "icon": "terminal",
      "overrideName": true,
      "path": "/bin/bash",
      "source": "bash"
    }
  },
  "terminal.integrated.profiles.osx": {
    "pytmp": {
      "args": [
        "-c",
        "source ${workspaceFolder}/.vscode/setup ${workspaceFolder} && exec zsh"
      ],
      "color": "terminal.ansiCyan",
      "env": {},
      "icon": "terminal",
      "overrideName": true,
      "path": "/bin/zsh",
      "source": "zsh"
    }
  },
  "terraform.experimentalFeatures.validateOnSave": true,
  "terraform.languageServer.enable": true,
  "workbench.iconTheme": "material-icon-theme"
}
